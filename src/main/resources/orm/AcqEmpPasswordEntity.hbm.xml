<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.acq.users.entity.AcqEmpPasswordEntity" table="acq_webuser_password" catalog="acquiro_ver1">
        <id name="empId" type="java.lang.Long">
            <column name="EMPID" length="20" />
        </id>       
         <property name="passwd1" type="string">
            <column name="PASSWORD1" length="75" not-null="false" />
        </property>       
        <property name="passwd2" type="string">
            <column name="PASSWORD2" length="75" not-null="false" />
        </property>   
        <property name="passwd3" type="string">
            <column name="PASSWORD3" length="75" not-null="false" />
        </property>   
        <property name="lastUpdatedOn" type="string">
            <column name="LASTUPDATED" length="75" not-null="false" />
        </property>        
    </class>
</hibernate-mapping>
