<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.acq.users.entity.AcqInventoryDeviceEntity" table="acq_terminal_summary" catalog="acquiro_ver1">
        <id name="inventoryDeviceId" type="java.lang.Long">
            <column name="id" />
            <generator class="identity" />
        </id>        
        <property name="modelName" type="string">
            <column name="name" length="45" not-null="true" />
        </property>
        <property name="inventorySerialNo" type="string">
            <column name="serialno" length="45" not-null="true" />
        </property>
        <property name="status" type="string">
            <column name="status" length="20" not-null="true" />
        </property>
        <property name="acquirer" type="string">
            <column name="AQUIRER_CODE" length="50" not-null="true" />
        </property>
        <property name="dateTime" type="string">
            <column name="DATETIME" length="50" not-null="true" />
        </property>
         <property name="batterySerialNo" type="string">
            <column name="batteryserialno" length="50" not-null="true" />
        </property>
         <property name="adapterSerialNo" type="string">
            <column name="ADAPTERSERIALNO" length="50" not-null="true" />
        </property>
         <property name="dsaList" type="string">
            <column name="dsalist" length="50" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
